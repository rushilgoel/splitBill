<link href="{{path}}/style/main.min.css" rel="stylesheet" type="text/css">
<script>
  ordrin = typeof ordrin==="undefined"?{}:ordrin;
  ordrin.init = ordrin.init===undefined?{}:ordrin.init;
  ordrin.init.rid = {{rid}};
  {{#render}}
    ordrin.init.render = true;
  {{/render}}
  {{^render}}
  ordrin.init.render = false;
  {{/render}}
  ordrin.init.page = "confirm";
  {{#data}}
    ordrin.init.menu = {{{this}}};
  {{/data}}
  ordrin.init.address = {{{address}}};
  ordrin.init.deliveryTime = {{{deliveryTime}}};
  ordrin.init.trayString = "{{tray}}";
  ordrin.init.tip = {{tip}};
  ordrin.restaurantUrl = ordrin.orderUrl =  window.location.protocol + "//" + window.location.host +"{{path}}/api";
  (function(){
    var ow = document.createElement('script'); ow.type = 'text/javascript'; ow.async = true;
    ow.src = '{{path}}/script/mustard.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ow, s);  
  })();
</script>
